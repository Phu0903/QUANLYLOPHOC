    <TabbedPage.Children>
        <NavigationPage Title="DS lớp học" IconImageSource="presentation">
            <x:Arguments>
                <tabbedbottom1:ListClass></tabbedbottom1:ListClass>
            </x:Arguments>
        </NavigationPage>
        <!--<NavigationPage Title="DS học sinh" IconImageSource="group">
            <x:Arguments>
                <tabbedbottom:MainPage></tabbedbottom:MainPage>
            </x:Arguments>
        </NavigationPage>-->

        <NavigationPage Title="Cá nhân" IconImageSource="user.png">
            <x:Arguments>
                <tabbedbottom1:DetailsInfor></tabbedbottom1:DetailsInfor>
            </x:Arguments>
        </NavigationPage>
    </TabbedPage.Children>
</TabbedPage>